# .env.example

# The port the backend should run on
PORT=5000

# MongoDB connection URI (replace with actual MongoDB credentials)
MONGO_URI=mongodb+srv://xyzxyz:<db_password>@cluster0.xe23y.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0

# JWT Secret for signing and verifying tokens (replace with a secure value)
JWT_SECRET=<a long string>

# API Key for external services (replace with actual key)
API_KEY=your-api-key-here

# Secure session secret for handling user sessions (replace with a secure value)
SESSION_SECRET=your-secure-session-secret-key